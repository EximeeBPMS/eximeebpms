<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions"
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
             xmlns:eximeebpms="http://eximeebpms.org/schema/1.0/bpmn"
             targetNamespace="Examples">

  <process id="AsyncContinuation" isExecutable="true">

    <startEvent id="start" />

    <sequenceFlow id="flow1" sourceRef="start" targetRef="miTask" />

	<serviceTask id="miTask" name="multi instance task" eximeebpms:expression="${true}">
		<multiInstanceLoopCharacteristics isSequential="true" eximeebpms:asyncBefore="true" eximeebpms:asyncAfter="true">
 			<loopCardinality>5</loopCardinality>
		</multiInstanceLoopCharacteristics>
	</serviceTask>
	
	<sequenceFlow id="flow2" sourceRef="miTask" targetRef="end" />
    
    <endEvent id="end" />

  </process>

</definitions>