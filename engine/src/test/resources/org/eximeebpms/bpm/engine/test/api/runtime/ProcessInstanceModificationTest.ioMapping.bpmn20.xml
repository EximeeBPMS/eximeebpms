<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions" xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:eximeebpms="http://eximeebpms.org/schema/1.0/bpmn" targetNamespace="Examples">

  <process id="ioMappingProcess" isExecutable="true">

    <startEvent id="theStart" />
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="task1" />
    <userTask id="task1" />
    <sequenceFlow id="flow2" sourceRef="task1" targetRef="task2"/>
    <userTask id="task2" >
      <extensionElements>
        <eximeebpms:inputOutput>
          <eximeebpms:inputParameter name="inputMappingExecuted">${true}</eximeebpms:inputParameter>
          <eximeebpms:outputParameter name="outputMappingExecuted">${true}</eximeebpms:outputParameter>
        </eximeebpms:inputOutput>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow3" sourceRef="task2" targetRef="theEnd" />
    <endEvent id="theEnd" />
  </process>

</definitions>