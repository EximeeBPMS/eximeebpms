<?xml version="1.0" encoding="UTF-8"?>
<definitions 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:eximeebpms="http://eximeebpms.org/schema/1.0/bpmn"
  targetNamespace="Examples">

  <process id="asyncStartEvent" isExecutable="true">
    <extensionElements>
	    <eximeebpms:executionListener event="start"
	      class="org.eximeebpms.bpm.engine.test.bpmn.async.AsyncListener"></eximeebpms:executionListener>
	  </extensionElements>
  
    <startEvent id="startEvent" eximeebpms:async="true" />
    
    <sequenceFlow sourceRef="startEvent" targetRef="task" />
      
    <userTask id="task"/>
    
    <sequenceFlow sourceRef="task" targetRef="endEvent" />
        
    <endEvent id="endEvent" />
    
  </process>

</definitions>
