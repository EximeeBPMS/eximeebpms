<?xml version="1.0" encoding="UTF-8"?>
<definitions id="definitions" 
  xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:eximeebpms="http://eximeebpms.org/schema/1.0/bpmn"
  targetNamespace="Examples">
  
  <process id="oneJobProcess" isExecutable="true">
  
    <startEvent id="theStart" />
    
    <sequenceFlow id="flow1" sourceRef="theStart" targetRef="asyncServiceTask" />
    
    <serviceTask id="asyncServiceTask" eximeebpms:asyncBefore="true"
      eximeebpms:class="org.eximeebpms.bpm.engine.test.api.runtime.migration.ConfigurableFailureDelegate"/>
    
    <sequenceFlow id="flow3" sourceRef="asyncServiceTask" targetRef="theEnd" />
    
    <endEvent id="theEnd" />
    
  </process>

</definitions>